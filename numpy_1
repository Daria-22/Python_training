import numpy as np
height = [1.75, 1.68, 1.71, 1.89, 1.79]
np_height = np.array(height) # create an array of heights from list
print(np_height)

weight = [65.4, 59.2, 63.6, 88.4, 86.7]
np_weight = np.array(weight) # create an array of weights fomr list
print(np_weight)

bmi = np_weight / np_height **2 
print(bmi)
